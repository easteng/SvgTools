<Window x:Class="WpfShapeSvgTestBox.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfShapeSvgTestBox"
        local:MainWindowSettings.Save="True"
        mc:Ignorable="d"
        Title="WPF SVG TestBox" Height="720" Width="860" WindowStartupLocation="CenterScreen">
    <DockPanel LastChildFill="True" Margin="3">
        <TabControl Margin="6" Grid.Row="0" Grid.Column="2">
            <TabItem Name="tabSvgInput" Header="Svg Input">
                <Frame Name="frameSvgInput" Source="SvgPage.xaml"/>
            </TabItem>
            <TabItem Name="tabXamlOutput" Header="Xaml Output" Visibility="Collapsed">
                <Frame Name="frameXamlOutput" Source="XamlPage.xaml"/>
            </TabItem>
        </TabControl>
    </DockPanel>
</Window>
